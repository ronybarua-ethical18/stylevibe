{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "lint",
        "check-types"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": [],
      "outputMode": "full"
    },
    "lint:fix": {
      "outputs": [],
      "outputMode": "full"
    },
    "check-types": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}